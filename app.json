{
  "expo": {
    "name": "FoodMood",
    "slug": "foodmood-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/plato-sonriente-tenedor.png",
    "scheme": "foodmood",
    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "04528f1e-2cb2-4252-8026-2d79a3c337d0"
      }
    },

    "android": {
      "package": "com.rominavilatta.foodmoodapp"
    },
    "owner": "rominavilatta"
  }
}
